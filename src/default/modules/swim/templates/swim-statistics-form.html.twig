{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}

{{ form.pace }}
{{ form.distance }}
{{ form.swim_id }}

<div class="actions">
    {{ form.actions }}
</div>

<table class="table table-striped swimmers">
    <thead>
    <tr>
    <th scope="col">Swim Title</th>
    <th scope="col">Date</th>
    <th scope="col">Pace</th>
    <th scope="col">Distance</th>

    </tr>
    </thead>
    <tbody id="stats">
    {% for stat in form.data %}
    <tr>
        <td>{{ stat.swim_name }}</td>
        <td>{{ stat.date }}</td>
        <td>{{ stat.pace }}</td>
        <td>{{ stat.distance }}</td>
    </tr>
    {% endfor %}
    </tbody>
</table>


